-static int __init checksetup(char *line)
-{
-	struct kernel_param *p;
-
-	p = &__setup_start;
-	do {
-		int n = strlen(p->str);
-		if (!strncmp(line,p->str,n)) {
-			if (p->setup_func(line+n))
-				return 1;
-		}
-		p++;
-	} while (p < &__setup_end);
-	return 0;
